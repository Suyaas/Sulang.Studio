<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="defBackgroundBrush" Color="#303030"></SolidColorBrush>
    <SolidColorBrush x:Key="lightBackgroundBrush" Color="#4e4e4e"></SolidColorBrush>
    <SolidColorBrush x:Key="highlightBackgroundBrush" Color="#2e4b59"></SolidColorBrush>

    <SolidColorBrush x:Key="defBorderBrush" Color="#4e4e4e"></SolidColorBrush>
    <SolidColorBrush x:Key="lightBroderBrush" Color="#666666"></SolidColorBrush>
    <SolidColorBrush x:Key="highlightBroderBrush" Color="#26a0da"></SolidColorBrush>
    
    <SolidColorBrush x:Key="defTextBrush" Color="#c0c0c0"></SolidColorBrush>
    <SolidColorBrush x:Key="lightTextBrush" Color="#dedede"></SolidColorBrush>
    <SolidColorBrush x:Key="highlightTextBrush" Color="#efefef"></SolidColorBrush>

</ResourceDictionary>
